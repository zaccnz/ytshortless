{
    "manifest_version": 3,
    "name": "YT Shortless",
    "version": "1.0.0",
    "options_page": "src/options/options.html",
    "content_scripts": [
        {
            "matches": [
                "https://youtube.com/*",
                "https://*.youtube.com/*"
            ],
            "js": [
                "src/contentscript.ts"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}